{
	"name": "baraja-core/gravatar",
	"description": "Simple PHP API provider for getting user Gravatar and user description.",
	"homepage": "https://github.com/baraja-core/gravatar",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": "^8.0",
		"nette/utils": "^3.0",
		"nette/caching": "^3.1"
	},
	"require-dev": {
		"tracy/tracy": "^2.8",
		"phpstan/phpstan": "^0.12.94",
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan-nette": "^0.12.14",
		"phpstan/phpstan-deprecation-rules": "^0.12.6",
		"phpstan/phpstan-strict-rules": "^0.12.10",
		"spaze/phpstan-disallowed-calls": "^1.1",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
